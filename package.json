{
  "name": "k8s-types",
  "version": "1.0.0",
  "description": "Strong typing for Kubernetes objects",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npx jest --coverage",
    "build": "rm -rf dist && npx tsc --outDir dist --sourceMap",
    "lint": "npx tslint --project tslint.json -t verbose",
    "prepublishOnly": "npm run lint && npm run build && npm version patch",
    "postpublish": "git push",
    "start": "node -r ts-node/register src/index.ts",
    "watch": "npx nodemon"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/yaml": "^1.0.2",
    "prettier": "^1.18.2",
    "yaml": "^1.6.0"
  },
  "types": "./src/index.ts",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.8",
    "@types/rimraf": "^2.0.2",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-morph": "^1.3.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  }
}
